{
  "window.zoomLevel": 0,
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}${separator}${appName}",
  "editor.fontFamily": "\"Liga Source Code Variable\", SourceHanCodeJP-Normal, SauceCodePowerline, Menlo, Monaco, 'Courier New', monospace",
  "editor.lineHeight": 20,
  "editor.wordWrap": "on",
  "editor.rulers": [80, 120],
  "files.insertFinalNewline": true,
  "workbench.iconTheme": "material-icon-theme",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.otherModesKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["<C-n>"],
      "after": [],
      "commands": [
        {
          "command": ":nohl"
        }
      ]
    }
  ],
  "vim.leader": "<space>",
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "ruby.lint": {
    "reek": false,
    "rubocop": true,
    "ruby": {
      "unicode": true //Runs ruby -wc -Ku
    },
    "fasterer": false,
    "debride": false,
    "ruby-lint": true
  },
  "haskell.ghcMod.executablePath": "stack",
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true
  },
  "[ruby]": {
    "editor.tabSize": 2
  },
  "[rust]": {
    "editor.formatOnSave": true
  },
  "[swift]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.tabSize": 2
  },
  "tslint.autoFixOnSave": true,
  "gitlens.keymap": "alternate",
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.historyExplorer.enabled": true,
  "gitlens.codeLens.enabled": false,
  "php-cs-fixer.onsave": true,
  "php-cs-fixer.rules": "@PSR2",
  "php-cs-fixer.config": ".php_cs;.php_cs.dist",
  "php-cs-fixer.formatHtml": false,
  "editor.formatOnSave": true,
  "sourcekit-lsp.serverPath": "/Users/hirofumiwakasugi/ghq/github.com/apple/sourcekit-lsp/.build/x86_64-apple-macosx/debug/sourcekit-lsp",
  "workbench.colorTheme": "Dracula",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.rendererType": "dom",
  "sourcekit-lsp.trace.server": "messages",
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "sourcekit-lsp.toolchainPath": "/Library/Developer/Toolchains/swift-4.2-DEVELOPMENT-SNAPSHOT-2018-10-30-a.xctoolchain",
  "editor.renderControlCharacters": true
}
